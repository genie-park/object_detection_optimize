{
  "source_model": {
    "model_name": "faster_rcnn_resnet101_coco",
    "output_dir": "data/models"
  },
  "optimization_config": {
    "use_trt": true,
    "precision_mode": "FP32",
    "force_nms_cpu": true,
    "replace_relu6": true,
    "remove_assert": true,
    "max_batch_size": 1,
    "override_resizer_shape": [600,600],
    "output_path" : "optimized/fasterRCNN_fp32.pb"
  },
  "benchmark_config": {
    "images_dir": "data/dataset/val2017",
    "annotation_path": "data/dataset/annotations/instances_val2017.json",
    "batch_size": 1,
    "num_images": 4096,
    "image_shape": [600, 600],
    "output_path": "stats/faster_rcnn_coco_trt_fp16.json"
  },
  "assertions": [
    "statistics['map'] > (0.268 - 0.005)"
  ]
}
